{
  "name": "edwardstock/yii2-queue",
  "description": "yii2 universal queue component",
  "type": "yii2-extension",
  "version": "1.0.0",
  "keywords": [
    "yii2",
    "extension",
    "queue",
    "rabbitmq",
    "redis"
  ],
  "license": "BSD-4-Clause",
  "authors": [
    {
      "name": "Eduard Maximovich",
      "email": "edward.vstock@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0.0",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "ext-shmop": "*",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-redis": "*",
    "jeremeamia/SuperClosure": "^2.2.0",
    "php-amqplib/php-amqplib": "~2.6",
    "edwardstock/forker": "^1.0.1",
    "symfony/process": "^3.1.2",
    "klaussilveira/simple-shm": "^1.0.0",
    "react/socket": "^0.4.3"
  },
  "require-dev": {
    "codeception/codeception": "*",
    "codeception/phpbuiltinserver": "*",
    "codeception/specify": "*",
    "phpunit/phpunit": "*",
    "codeception/verify": "*",
    "yiisoft/yii2-codeception": "*"
  },
  "autoload": {
    "psr-4": {
      "edwardstock\\queue\\": "src/"
    }
  }
}
